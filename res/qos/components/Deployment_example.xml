<!-- 
=======================================================================
    							Component QoS				
=======================================================================
RTI Connext DDS User QoS Profiles

Configure the component specific "Participant" Qos profile settings
such as: transports, discovery, monitoring, resources, etc.
	    	 
The following naming convention is used to match the profiles names to
component names:
          name="User.Selected.Component.<component_name>"

Example:
	For a component "square", the component profile is named:
		  name="User.Selected.Component.square" 
		  
Author: Rajive Joshi, Real-Time Innovations Inc. Copyright (C) 2017.
-->
<dds
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://community.rti.com/schema/5.3.0/rti_dds_qos_profiles.xsd"
	version="5.3.0">

	<qos_library name="ParticipantQoSLib">

		<qos_profile
			name="User.Selected.Component.square_pub_sub"
			base_name="User.Selected.Participant">
		</qos_profile>

		<qos_profile
			name="User.Selected.Component.square_pub"
			base_name="User.Selected.Participant">
		</qos_profile>

		<qos_profile
			name="User.Selected.Component.square_sub"
			base_name="User.Selected.Participant">
		</qos_profile>

		<qos_profile
			name="User.Selected.Component.circle_pub_triangle_pub"
			base_name="User.Selected.Participant">
		</qos_profile>

		<qos_profile
			name="User.Selected.Component.square_sub_circle_sub"
			base_name="User.Selected.Participant">
		</qos_profile>						
	</qos_library>
</dds>
